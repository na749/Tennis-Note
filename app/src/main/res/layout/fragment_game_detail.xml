<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Practice.PracticeDetailFragment">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/scrollView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="10dp">

            <TextView
                android:id="@+id/GameEdit1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:padding="10dp"
                android:text="日付"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGameDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="11/11"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="会場"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGamePlace"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="有明の森"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="スコア"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGameScore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="8-3"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:padding="10dp"
                android:text="試合の内容"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGameToday"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="よかった"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:padding="10dp"
                android:text="試合の反省"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGameReflection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="明日に生かす"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/GameEdit8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:padding="10dp"
                android:text="次回意識すること"
                android:textColor="@color/TextColor"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtGameNext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:text="フォアを強くする"
                android:textColor="@color/TextColor"
                android:textSize="18sp" />
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>